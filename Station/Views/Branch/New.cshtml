@using Station.Models
@model Point
{
    Layout = "_Layout";
}<div class="app-content content">
	<div class="content-overlay"></div>
	<div class="content-wrapper">
		<div class="content-header row">
			<div class="content-header-left col-12 mb-2 mt-1">
				<div class="breadcrumbs-top">
					<div class="breadcrumb-wrapper d-none d-sm-block">
						<ol class="breadcrumb p-0 mb-0 pl-1">
							<li class="breadcrumb-item">
								<a href="index.html"><i class="bx bx-home-alt"></i></a>
							</li>
							<li class="breadcrumb-item">
								<a href="#">الفروع</a>
							</li>
							<li class="breadcrumb-item active">
صفحة الاضافة							</li>
						</ol>
					</div>
				</div>
			</div>
		</div>
		<div class="card">
			<div class="card-header">
				<h3 class="card-title">اضافة فرع</h3>
			</div>
			<div class="card-body mt-2">
				<form asp-action="New" class="wizard-horizontal wizard clearfix" role="application" id="steps-uid-0">
					

								<!-- Step 1 end-->
								<!-- body content step 1 -->
								<fieldset id="steps-uid-0-p-0" role="tabpanel" aria-labelledby="steps-uid-0-h-0" class="body">

									<div class="row">

                                                <div class="col-sm-6">
                                                    <div class="form-group">

												<label asp-for="NamePoint"> الاسم </label>

												<input asp-for="NamePoint" class="form-control" id="lastName12" placeholder="ادخل اسم الفرع">
											</div>
										</div>
										<div class="col-sm-6">
											<div class="form-group">
												<label asp-for="Date">تاريخ الاضافة </label>
												<input ASP-for="Date" class="form-control" id="emailAddress1" placeholder="ادخل تاريخ الاضافة">
												<span asp-validation-for="Date" class="text-danger"></span>

											</div>
										</div>

										<div class="col-sm-6">
											<div class="form-group">
								<button type="submit" class="btn btn-primary glow mr-1 mb-1" style="width:150px">حفظ</button>

								                                 &nbsp;
								<a asp-action="Index" asp-controller="Branch" class="btn btn-light glow mr-1 mb-1" style="width:150px"> رجوع</a>
							
								
								
								</div>
								</div>
								</div>

																</fieldset>

				</form>
			</div>
		</div>
	</div>
</div>
                            @section Scripts {
<partial name="_ValidationScriptsPartial"></partial>
}
